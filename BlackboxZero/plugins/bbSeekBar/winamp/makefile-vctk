
CPP=cl.exe
OUTDIR=.\Release
INTDIR=.\Release

ALL : "$(OUTDIR)\winamp.dll"

"$(OUTDIR)" : 
	if not exist "$(OUTDIR)/$(NULL)" mkdir "$(OUTDIR)"

LINK32=link.exe
LINK32_FLAGS=blackbox.lib msvcrt.lib kernel32.lib user32.lib \
	gdi32.lib comdlg32.lib shell32.lib shlwapi.lib comctl32.lib \
	/subsystem:windows /nodefaultlib /dll /out:"$(OUTDIR)\winamp.dll"
LINK32_OBJS= "$(INTDIR)\winamp.obj" \

"$(OUTDIR)\winamp.dll" : "$(OUTDIR)" $(LINK32_OBJS)
    $(LINK32) @<<
  $(LINK32_FLAGS) $(LINK32_OBJS)
<<

CPP_PROJ=/nologo /MT /W3 /GX /O2 /D "WIN32" /D "NDEBUG" /D "_WINDOWS" /D "_MBCS" /Fo"$(INTDIR)\\" /c 

.cpp{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_PROJ) $< 
<<

SOURCE=.\winamp.cpp

"$(INTDIR)\winamp.obj" : $(SOURCE) "$(INTDIR)"
